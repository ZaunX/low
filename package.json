{
  "name": "low",
  "version": "1.0.1",
  "description": "A low code system framework",
  "main": "lib/index.js",
  "scripts": {
    "test": "SECRETS=\"secrets.json\" jest"
  },
  "keywords": [
    "lowcode",
    "low",
    "low",
    "code"
  ],
  "author": "Matthew Wilkes",
  "license": "GPL-3.0",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "globals": {
      "SECRETS": "secrets.json"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {}
}
